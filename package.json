{
  "name": "eclypsys-ecommerce-proj",
  "version": "0.0.1",
  "type": "module",
  "scripts": {

    "dev": "bun run tailwind && bun run serve'",
    "serve": "concurrently \"bun run tailwind:watch\" \"vite dev\"",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "tailwind:watch": "tailwindcss -i ./src/app.css -o ./static/output.css --watch",
    "tailwind": "tailwindcss -i ./src/app.css -o ./static/output.css "
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.0.0",
    "vite": "^5.0.3"
  },
  "dependencies": {
    "concurrently": "^9.1.0",
    "svelte-icons": "^2.1.0"
  }
}